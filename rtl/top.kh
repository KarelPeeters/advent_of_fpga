import common.[char, Point, axi_stream];
import string.parse;

const M = 10**5;

module top ports(
    clk: in clock,
    rst: in async bool,

    sync(clk, rst) {
        input: interface axi_stream(char).input,
        output: interface axi_stream(Point(uint(0..M))).output,
    }
) {
    instance parse(M) ports(clk, rst, input, output);
}
