import common.[char, Point, axi_stream];

module top ports(
    clk: in clock,
    rst: in async bool,
    sync(clk, rst) {
        input: interface axi_stream(char).input,
    }
) {
    reg input_ready: bool = false;

    clocked(clk, async rst) {
        input_ready = false;
    }

    comb {
        input.ready = input_ready;
    }
}
